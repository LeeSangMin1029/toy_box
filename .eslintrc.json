{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "rules": {
        "eqeqeq":"warn",
        "curly":"warn",
        "no-unused-vars": "warn",
        "no-unused-expressions":"warn"
    }
}
